{
  "name": "@mimivibe/backend",
  "version": "1.0.0",
  "description": "MiMiVibes Backend Services - AI Processing, Queue System, Background Jobs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "worker": "node dist/src/workers/tarot-worker.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t mimivibe-backend .",
    "docker:run": "docker run -p 3001:3001 mimivibe-backend",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:down": "prisma migrate reset --force",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:status": "prisma migrate status",
    "db:reset": "prisma migrate reset --force && prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bullmq": "^4.15.4",
    "redis": "^4.6.12",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "openai": "^4.24.1",
    "@google/generative-ai": "^0.2.1",
    "langchain": "^0.2.0",
    "@langchain/openai": "^0.2.0",
    "@langchain/google-genai": "^0.2.0",
    "@langchain/langgraph": "^0.2.0",
    "figlet": "^1.7.0",
    "boxen": "^7.1.1",
    "ora": "^7.0.1",
    "dompurify": "^3.0.5",
    "jsdom": "^23.0.1",
    "date-fns": "^2.30.0",
    "crypto-js": "^4.2.0",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/crypto-js": "^4.2.1",
    "@types/node-cron": "^3.0.11",
    "@types/figlet": "^1.5.8",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "prisma": "^5.7.1",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "tarot",
    "ai",
    "queue",
    "backend",
    "bullmq",
    "redis",
    "langchain",
    "openai",
    "gemini"
  ],
  "author": "MiMiVibes Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojisejr/mimivibe-ai.git",
    "directory": "backend"
  }
}